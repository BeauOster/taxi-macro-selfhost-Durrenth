class BaseMap {

    Name := "BaseMap"
    MapImage := "Image Capture here"
    
    TryPlacingUnits() {
        SpiralPlacement(true)
    }

    RunInstructions() {
        ; Default keyboard instructions here
    }

    HoldKey(key, duration) {
        SendInput "{" key " down}"
        Sleep(duration)
        SendInput "{" key " up}"
    }

    OnSpawnSetupGlobal() {
        SendInput ("{Tab}")
        LookDown()
        Sleep 200
        TPtoSpawn()
    }

}

